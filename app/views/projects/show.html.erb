<div class="project_container">
    <div class="project_banner">
        <h2><%= @project.title %></h2>
    </div>

    <div class="modalsong hidden">
      <div>
        <%= @project.title %>
      </div>
      <div>
        <%= simple_form_for(@project) do |f| %>
          <%= f.input :song, as: :file %>
          <%= f.submit %>
        <% end %>
      </div>
    </div>

    <div class="project_body">
        <div class="voice_record" id="waveform">


        </div>

        <div class="notes">

        </div>
    </div>
    <div class="project_footer">

    </div>
</div>
